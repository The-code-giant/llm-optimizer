{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --transpile-only src/index.ts",
    "dev:debug": "nodemon --inspect=0.0.0.0:9229 --exec ts-node --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:safe": "echo 'Running database migrations...' && drizzle-kit migrate || echo 'Migrations completed or already up to date'",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio --port 4983",
    "db:seed": "ts-node --transpile-only src/db/seed.ts",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cleanup:pinecone": "node src/utils/cleanupPineconeIndexes.js",
    "test:namespace": "node src/utils/testNamespace.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@clerk/backend": "^2.3.1",
    "@pinecone-database/pinecone": "^6.1.2",
    "@sentry/node": "^9.32.0",
    "@types/axios": "^0.14.4",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.56.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.27.1",
    "drizzle-orm": "^0.44.2",
    "express": "^5.1.0",
    "express-winston": "^4.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.11.0",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "stripe": "^18.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.4",
    "@types/pg": "^8.15.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
